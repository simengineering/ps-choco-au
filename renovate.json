{
  "baseBranches": ["main"],
  "extends": ["docker"],
  "docker": {
    "registryUrls": ["https://ocir.us-gov-ashburn-1.oci.oraclegovcloud.com"],
    "enabled": true,
  "customManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": ["MONO_VERSION=\"(?<currentValue>.*?)\""],
      "depNameTemplate": "docker.io/library/mono",
      "datasourceTemplate": "docker"
    }
  ]
}
